{
    "name": "@rosinfo.tech/boilerplate_frontend_ssr",
    "description": "Rosinfotech Boilerplate Frontend SSR",
    "version": "0.0.3",
    "author": "rosinfo.tech",
    "license": "UNLICENSED",
    "scripts": {
        "build": "dotenv -e envs/.env.production -- next build",
        "dev": "dotenv -e envs/.env.development -- next dev --turbo",
        "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "husky:pre-commit": "",
        "husky:pre-push": "npm run format:fix && npm run lint:fix && npm run stylelint:fix",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prepare": "husky",
        "start": "next start",
        "stylelint": "stylelint \"**/*.{css,scss}\"",
        "stylelint:fix": "stylelint \"**/*.{css,scss}\" --fix",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@ant-design/nextjs-registry": "^1.3.0",
        "@tailwindcss/postcss": "^4.1.18",
        "antd": "^6.1.1",
        "autoprefixer": "^10.4.23",
        "next": "^15.1.3",
        "postcss": "^8.5.6",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "tailwindcss": "^4.1.18"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@rosinfo.tech/eslint-config-import": "^0.0.9",
        "@rosinfo.tech/eslint-config-javascript": "^0.0.32",
        "@rosinfo.tech/eslint-config-react": "^0.0.20",
        "@rosinfo.tech/eslint-config-typescript": "^0.0.27",
        "@rosinfo.tech/prettier-config-standard": "^0.0.10",
        "@rosinfo.tech/stylelint-config-standard": "^0.0.8",
        "@types/node": "^22.10.2",
        "@types/react": "^19.0.1",
        "@types/react-dom": "^19.0.2",
        "dotenv-cli": "^11.0.0",
        "eslint": "^9",
        "eslint-config-next": "^15.1.3",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "postcss-import": "^16.1.1",
        "prettier": "^3.7.4",
        "stylelint": "^16.26.1",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.50.1"
    },
    "engines": {
        "node": ">=18.17.0",
        "npm": ">=9.0.0"
    },
    "private": true,
    "type": "module"
}
